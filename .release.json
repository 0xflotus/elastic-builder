{
  "verbose": true,
  "buildCommand": "npm run build",
  "changelogCommand": "git log --pretty=format:\"* %s (%h)\" [REV_RANGE]",
  "requireCleanWorkingDir": true,
  "src": {
    "tagName": "v%s",
    "beforeStartCommand": "npm run check"
  },
  "dist": {
    "baseDir": false
  },
  "github": {
    "release": true
  },
  "npm": {
    "publish": true
  }
}
