{
  "verbose": true,
  "pkgFiles": ["package.json", "package-lock.json"],
  "buildCommand": "npm run build",
  "requireCleanWorkingDir": true,
  "src": {
    "commitMessage": "chore(package): Release %s",
    "tagName": "v%s",
    "beforeStartCommand": "npm run check"
  },
  "dist": {
    "baseDir": false
  },
  "github": {
    "release": true
  },
  "npm": {
    "publish": false
  }
}
