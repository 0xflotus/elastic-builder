{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": 6,
          "browsers": [">1%"]
        }
      }
    ]
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": true,
        "polyfill": false,
        "regenerator": false,
        "moduleName": "babel-runtime"
      }
    ]
  ],
  "ignore": ["test/**/*.js"]
}
