{
  "name": "elastic-builder",
  "version": "0.1.0",
  "description": "A JavaScript implementation of the elasticsearch Query DSL",
  "main": "lib/index.js",
  "files": [
    "browser/",
    "lib/",
    "src/",
    "repl.js"
  ],
  "scripts": {
    "build:babel": "babel src --out-dir lib",
    "build:umd": "webpack lib browser/elastic-builder.min.js",
    "build:docs": "documentation build src/index.js -o docs -f html --github --name elastic-builder",
    "build": "npm run build:babel && npm run build:umd && npm run build:docs",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "lint-staged": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1",
    "preversion": "npm run lint && npm run build && npm run git-commit",
    "postversion": "npm run git-push",
    "git-commit": "git add docs browser && git commit -m \"Commit built files\"",
    "git-push": "cross-env \"git push origin master && git push origin v$npm_package_version\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudo-suhas/elastic-builder.git"
  },
  "keywords": [
    "elasticsearch",
    "search",
    "elasticjs",
    "elastic search",
    "elastic-builder",
    "ejs"
  ],
  "author": "Suhas Karanth <sudo.suhas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sudo-suhas/elastic-builder/issues"
  },
  "homepage": "https://github.com/sudo-suhas/elastic-builder#readme",
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": "lint-staged",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^4.0.0",
    "documentation": "^4.0.0-beta.19",
    "eslint": "^3.18.0",
    "lint-staged": "^3.4.0",
    "pre-commit": "^1.2.2",
    "webpack": "^2.3.3"
  },
  "dependencies": {
    "lodash.concat": "^4.5.0",
    "lodash.filter": "^4.6.0",
    "lodash.has": "^4.5.2",
    "lodash.hasin": "^4.5.2",
    "lodash.head": "^4.0.1",
    "lodash.invokemap": "^4.6.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isnil": "^4.0.0",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.map": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.reduce": "^4.6.0"
  }
}
